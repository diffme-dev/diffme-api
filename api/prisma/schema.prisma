datasource db {
    // could be postgresql or mysql
    provider = "mongodb"
    url      =  env("MONGODB_URI")
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
}

model Post {
    id        String   @default(cuid()) @id
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    title     String
    published Boolean
    desc      String?
}